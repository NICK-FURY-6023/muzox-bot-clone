{
  "name": "muzox-bot-clone",
  "version": "3.1.0",
  "description": "Muzox discord bot v2 typescript support ",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project tsconfig.json",
    "start": "node .",
    "restart": "nodemon -w Src -e ts --exec \"npm run build && nodemon dist/index.js\"",
    "dev": "rimraf dist && nodemon -w Src -e ts --exec \"npm run build && nodemon --enable-source-maps dist/index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "nick fury",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^20.12.5",
    "nodemon": "^3.1.0",
    "prisma": "^5.12.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "@prisma/client": "^5.12.1",
    "@top-gg/sdk": "^3.1.6",
    "canvas": "^2.11.2",
    "chalk": "^5.3.0",
    "colors": "^1.4.0",
    "crypto": "^1.0.1",
    "discord-hybrid-sharding": "github:meister03/discord-hybrid-sharding",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.5",
    "fs": "^0.0.1-security",
    "Logger": "^0.0.0",
    "mongoose": "^8.3.0",
    "node-superfetch": "^0.3.4",
    "os-utils": "^0.0.14",
    "pretty-ms": "^9.0.0",
    "shoukaku": "github:Deivu/Shoukaku",
    "typescript": "^5.4.4",
    "ws": "^8.16.0"
  }
}

